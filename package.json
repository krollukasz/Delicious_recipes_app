{
  "name": "delicious_recipes_app",
  "version": "1.0.0",
  "description": "App for searching recipes",
  "main": "index.js",
  "scripts": {
    "init-project": "npm install && npm-run-all init:*",
    "init:dirs": "mkdirp src/sass src/css src/images src/js",
    "init:files": "touch README.md src/index.html src/sass/style.scss src/js/index.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "dev": "webpack-dev-server  --config webpack.dev.js --open",
    "build": "webpack --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krollukasz/Delicious_recipes_app.git"
  },
  "author": "Lukasz Krol",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/krollukasz/Delicious_recipes_app/issues"
  },
  "homepage": "https://github.com/krollukasz/Delicious_recipes_app#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mkdirp": "^1.0.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.4",
    "regenerator-runtime": "^0.13.3"
  }
